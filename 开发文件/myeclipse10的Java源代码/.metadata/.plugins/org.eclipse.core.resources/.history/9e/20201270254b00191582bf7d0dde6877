package dao;

/**
 * AbstractText1 entity provides the base persistence definition of the Text1
 * entity. @author MyEclipse Persistence Tools
 */

public abstract class AbstractText1 implements java.io.Serializable {

	// Fields

	private Text1Id id;
	private Text1UserName username;

	// Constructors

	public Text1UserName getUsername() {
		return username;
	}

	public void setUsername(Text1UserName username) {
		this.username = username;
	}

	/** default constructor */
	public AbstractText1() {
	}

	/** full constructor */
	public AbstractText1(Text1Id id) {
		this.id = id;
	}

	// Property accessors

	public Text1Id getId() {
		return this.id;
	}

	public void setId(Text1Id id) {
		this.id = id;
	}

}