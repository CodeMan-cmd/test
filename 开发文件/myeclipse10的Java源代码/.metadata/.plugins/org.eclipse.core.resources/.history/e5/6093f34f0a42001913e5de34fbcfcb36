package dao;import java.util.List;import javax.annotation.Resource;import org.apache.ibatis.session.SqlSession;import org.mybatis.spring.SqlSessionTemplate;import org.mybatis.spring.support.SqlSessionDaoSupport;import org.springframework.stereotype.Repository;import pojo.User;import utils.MyBatisUtil;import utils.Page;/** * 实现类，实现UserMapper接口 * @author Mister He * */@Repository("userDao")public class UserDaoImpl implements UserMapper {	@Resource	private SqlSessionTemplate sqlSessionTemplate;		public SqlSessionTemplate getSqlSession() {		return sqlSessionTemplate;	}	public void setSqlSession(SqlSessionTemplate sqlSession) {		this.sqlSessionTemplate = sqlSession;	}		public List<User> selectAll(User u){		return sqlSessionTemplate.selectList("dao.UserMapper.selectAll",u);	}	public List<User> selectByPR(User u, Page p) {		return null;	}	public int selectCountByUser(User u) {		return 0;	}	public int addUser(User a) {		return sqlSessionTemplate.insert("dao.UserMapper.selectAll",a);	}		}