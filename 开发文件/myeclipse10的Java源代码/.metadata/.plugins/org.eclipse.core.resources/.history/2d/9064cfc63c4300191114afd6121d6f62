package service;import java.util.List;import org.apache.ibatis.annotations.Param;import pojo.User;import utils.Page;/** * 业务逻辑层 * @author Mister He * */public interface UserService {	/**	 * 添加功能	 * @param u用户对象	 * @return	 */	/*public int addUser(User u);*/	public List<User> selectUser(User s);	/***	 * 动态查询 分页 俩条件	 * @param u 用户对象	 * @param p 分页工具对象	 * @return 用户信息的集合 	 */	public List<User> selectByPR(User u,Page p);	public int selectCountByUser(User u);	public int addUser(User a);}