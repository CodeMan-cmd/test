<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapperPUBLIC "-//mybatis.org//DTD Mapper 3.0//EN""http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="dao.UserMapper">	<select id="selectUser" parameterType="User" parameterMap="titi">		SELECT userCode,userName FROM smbms_user	</select>	<insert id="addUser">		INSERT INTO smbms_user(userName,userPassword)		VALUES(#{a.userName},#{a.userPassword})	</insert>	<resultMap type="User" id="titi">		<result property="userCode" column="userCode"></result>		<result property="userName" column="userName"></result>	</resultMap></mapper>