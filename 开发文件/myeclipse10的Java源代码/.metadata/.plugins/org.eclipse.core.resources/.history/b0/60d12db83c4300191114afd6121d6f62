package service;import java.util.List;import javax.annotation.Resource;import org.springframework.stereotype.Service;import dao.UserMapper;import pojo.User;import utils.Page;@Servicepublic class UserServiceImpl implements UserService {		@Resource	private UserMapper userMapper;	// 封装	public UserMapper getUserDao() {		return userMapper;	}	public void setUserDao(UserMapper userDao) {		userMapper = userDao;	}	// 无参构造	public UserServiceImpl() {		super();	}	// 有参构造	public UserServiceImpl(UserMapper userDao) {		super();		userMapper = userDao;	}	// 调用dao层的方法	/*public List<User> selectUser() {	//List<User>	select=userMapper.selectUser(s);		//System.out.println(select.size());		return userMapper.selectUser();	}*/	public List<User> selectByPR(User u, Page p) {		p.setZongyeshu(selectCountByUser(u));		return userMapper.selectByPR(u, p);	}	public int selectCountByUser(User u) {		return userMapper.selectCountByUser(u);	}	public int addUser(User a) {		return userMapper.addUser(a);	}	public List<User> selectUser() {		List<User>	select=userMapper.selectUser(s);		//System.out.println(select.size());		return userMapper.selectUser();	}}