package tset;public class ConfigManager {	private static ConfigManager cm;	/**	 * 初始化操作，主要负责读取database.properties文件属性，连接数据库	 */	private ConfigManager	(){			}		/**	 * 声明一个静态内部类	 * @author Mister He	 *	 */	public static class getcm{		//创建一个静态常量		public static final ConfigManager cn= new ConfigManager();		}	public static ConfigManager getcm(){		cm = getcm.cn;		return cm;	}}