package dao;import java.util.List;import org.apache.ibatis.annotations.Param;import pojo.User;import utils.Page;public interface UserMapper {	/**	 * 添加功能	 * @param u用户对象	 * @return	 */	public int addUser(@Param("u") User u);	public List<User> selectAll(User u);	//动态查询 分页 俩条件	public List<User> selectByPR(@Param("u")User u,@Param("p")Page p);	}}